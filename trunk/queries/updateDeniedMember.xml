<query id="updateDeniedMember" action="update">
    <tables>
        <table name="member" />
    </tables>
    <columns>
        <column name="denied" var="denied" default='Y' />
    </columns>
    <conditions>
        <condition operation="equal" column="member_srl" var="member_srl" notnull="notnull" filter="number"/>
    </conditions>
</query>
